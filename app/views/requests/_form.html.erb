<%= simple_form_for(@request) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :available %>
    <%= f.association :user %>
    <%= f.association :song %>
    <%= f.association :venue %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
